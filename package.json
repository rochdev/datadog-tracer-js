{
  "name": "datadog-tracer",
  "version": "0.3.4",
  "description": "OpenTracing Tracer implementation for Datadog in JavaScript",
  "main": "src/index.js",
  "scripts": {
    "pbjs": "pbjs -t static-module src/propagation/state.proto > src/propagation/state.proto.js",
    "lint": "eslint .",
    "test": "eslint . && nyc --reporter=lcov --reporter=text mocha",
    "tdd": "mocha --watch",
    "codecov": "codecov"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.proto.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rochdev/datadog-tracer-js.git"
  },
  "keywords": [
    "datadog",
    "tracer",
    "opentracing",
    "apm"
  ],
  "author": "Roch Devost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rochdev/datadog-tracer-js/issues"
  },
  "homepage": "https://github.com/rochdev/datadog-tracer-js#readme",
  "dependencies": {
    "@protobufjs/eventemitter": "^1.1.0",
    "long": "^3.2.0",
    "opentracing": "^0.14.1",
    "performance-now": "^2.1.0",
    "protobufjs": "^6.8.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "codecov": "^2.2.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.3",
    "mocha": "^4.0.0",
    "nock": "^9.0.13",
    "nyc": "^11.0.2",
    "proxyquire": "^1.8.0",
    "safe-buffer": "^5.1.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0"
  }
}
